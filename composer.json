{
  "name": "javanile/php-imap2",
  "type": "library",
  "description": "Implement no-code input function for PHP",
  "keywords": [
    "input",
    "webrequest",
    "nocode",
    "lowcode",
    "web3"
  ],
  "homepage": "https://github.com/javanile/php-imap2",
  "license": "MIT",
  "authors": [
    {
      "name": "Francesco Bianco",
      "homepage": "https://github.com/francescobianco"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "Javanile\\Imap2\\": "src"
    },
    "files": [
      "bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Javanile\\Imap2\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "thanks": {
      "name": "javanile/webrequest",
      "url": "https://github.com/javanile/webrequest"
    }
  },
  "require": {
    "zbateson/mail-mime-parser": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  }
}
